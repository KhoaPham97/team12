            $("#Startdate").focus();
        }
        function Duedate() {
            $("#Duedate").focus();
        }
        $(function () {
          

            $("#Startdate").datepicker({
                numberOfMonths: 2,
                onSelect: function (selected) {
                    var dt = new Date(selected);
                    dt.setDate(dt.getDate() + 1);
                    $("#Duedate").datepicker("option", "minDate", dt);
                }
            });
            $("#Duedate").datepicker({
                numberOfMonths: 2,
                onSelect: function (selected) {
                    var dt = new Date(selected);
                    dt.setDate(dt.getDate() - 1);
                    $("#Startdate").datepicker("option", "maxDate", dt);
                }
            });
